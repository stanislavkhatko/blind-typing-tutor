export default class Keyboard {
    constructor() {
        this.root = $('.keyboard-container');
        this.keys = $('.keyboard-container .key');
        this.tools = $('.keyboard-toolbar .tool')
        this.keyTargetClass = 'key-target';
        this.righthandedSpace = 'righthand';

        this.keyboardMatrix = [
            {id: '1', en: '1', uk: '1',},
            {id: '2', en: '2', uk: '2',},
            {id: '3', en: '3', uk: '3',},
            {id: '4', en: '4', uk: '4',},
